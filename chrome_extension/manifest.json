{
  "manifest_version": 2,
  "name": "Guacamoly",
  "description": "This extension allows to add the current page as a Guacamoly thing.",
  "icons": { "128": "logo.png" },
  "version": "0.1.0",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}